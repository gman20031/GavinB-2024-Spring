#define CREATE_ARITHMETIC_OPERATOR(op) Vector3 operator+(const Vector3& lhs, const Vector3& rhs){ return ( { lhs.GetX() op rhs.GetX(), lhs.GetY() op rhs.GetY(), lhs.GetZ() op rhs.GetZ(), } ); }
